{
  "chat.tools.terminal.autoApprove": {
    "cargo fmt": true,
    "cargo clippy": true,
    "bun": true,
    "cargo check": true,
    "cargo tree": true,
    "true": true
  },
  "rust-analyzer.checkOnSave": true,
  "rust-analyzer.check.overrideCommand": [
    "cargo",
    "clippy",
    "--workspace",
    "--all-targets",
    "--all-features",
    "--message-format=json",
    "--",
    "-D",
    "warnings"
  ]
}